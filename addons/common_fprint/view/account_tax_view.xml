<?xml version="1.0" encoding="utf-8"?>
<odoo>
<record id="view_tax_group_pos" model="ir.ui.view">
        <field name="name">view.tax.group.pos</field>
        <field name="model">account.tax</field>
        <field name="inherit_id" ref="account.view_tax_form"/>

<?xml version="1.0" encoding="utf-8"?>
<odoo>

        <record id="sale_order_construction_name_form" model="ir.ui.view">
                <field name="name">sale.order.construction.name Form</field>
                <field name="model">sale.order.construction.name</field>
                <field name="type">form</field>
                <field name="arch" type="xml">
                    <form string="Construction Project Name">
                        <sheet>
                            <group>
                                <field name="order_ids" invisible="1"/>
                                <field name="construction_order_id" invisible="1"/>
                                <field name="email" invisible="1"/>
                                <field name="obiectiv" invisible="construction_order_id == False"/>
<!--                                <field name="construction_order_ids" invisible="construction_order_ids == []">-->
<!--                                    <list editable="bottom" create="false">-->
<!--                                        <field name="construction_order_id" invisible="1"/>-->
<!--                                        <field name="obiectiv"/>-->
<!--                                    </list>-->
<!--                                </field>-->
                            </group>
                        </sheet>
                        <footer>
                            <button name="execute" string="Execute" type="object" class="btn-primary"/>
                            <button string="Cancel" class="btn-secondary" special="cancel" />
                        </footer>
                    </form>
                </field>
        </record>

</odoo>

